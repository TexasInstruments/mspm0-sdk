<?xml version="1.0" encoding="UTF-8"?>
<projectSpec>
    <applicability>
        <when>
            <context
                deviceFamily="ARM"
                deviceId="Cortex M.MSPM0G3507"
            />
        </when>
    </applicability>

    <project
        title="Emeter_three_phase_ADS131M08MET_EVM_SW"
        name="Emeter_three_phase_ADS131M08MET_EVM_SW"
        configurations="Debug"
        toolChain="TICLANG"
        connection="TIXDS110_Connection.xml"
        device="Cortex M.MSPM0G3507"
        ignoreDefaultDeviceSettings="true"
        ignoreDefaultCCSSettings="true"
        products="MSPM0-SDK;sysconfig"

        compilerBuildOptions="
		    -I${PROJECT_ROOT}/ADS131M08MET_EVM/
            -I${PROJECT_ROOT}/modules/hal/
	        -I${PROJECT_ROOT}/modules/ADS131M08/
            -I${PROJECT_ROOT}/modules/metrology/
	        -I${PROJECT_ROOT}/modules/DLT645/
            -I${PROJECT_ROOT}
            -I${PROJECT_ROOT}/${ConfigName}
            -D__MSPM0G3507__
            -O2
            -I${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/third_party/CMSIS/Core/Include
            -I${COM_TI_MSPM0_SDK_INSTALL_DIR}/source
            -gdwarf-3
            -mcpu=cortex-m0plus
            -march=thumbv6m
            -mfloat-abi=soft
            -mthumb
        "
        linkerBuildOptions="
            -ldevice.cmd.genlibs
            -L${COM_TI_MSPM0_SDK_INSTALL_DIR}/source
            -L${PROJECT_ROOT}
            -Wl,--stack_size=512
            -L${PROJECT_BUILD_DIR}/syscfg            
            -Wl,--rom_model
            -Wl,--warn_sections
            -L${CG_TOOL_ROOT}/lib
            -llibc.a
        "
        enableSysConfigTool="true"
        sysConfigBuildOptions="
            --product ${COM_TI_MSPM0_SDK_INSTALL_DIR}/.metadata/product.json
            --compiler ticlang
        "
        sourceLookupPath="${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/ti/driverlib"
        description="Three phase solution on ADS131M08MET EVM hardware">

        <pathVariable name="SDK_ROOT" path="../../../../../../" scope="project"/>
        <pathVariable name="ORIG_PROJ_SOURCE_DIR" path="../../../../../../source/ti/energy_metrology/modules" scope="project"/>
        <pathVariable name="ORIG_PROJ_ROOT_DIR" path="../" scope="project"/>
        <property name="buildProfile" value="release"/>
        <property name="isHybrid" value="true"/>
        <file path="../main.c" openOnCreation="false" excludeFromBuild="false" action="copy"></file>
        <file path="../template.h" openOnCreation="false" excludeFromBuild="false" action="copy"></file>
        <file path="../ADS131M08MET_EVM.syscfg" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>   
        <file path="../ticlang/mspm0g1506.cmd" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../ADS131M08MET_EVM/ADS131M08MET_EVM.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="ADS131M08MET_EVM">
        </file>
        <file path="../ADS131M08MET_EVM/ADS131M08MET_EVM.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="ADS131M08MET_EVM">
        </file> 
        <file path="../ADS131M08MET_EVM/HW.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="ADS131M08MET_EVM">
        </file>
        <file path="../ADS131M08MET_EVM/HW.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="ADS131M08MET_EVM">
        </file> 
        <file path="../ADS131M08MET_EVM/irq_handler.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="ADS131M08MET_EVM">
        </file>
  
        <file path="ORIG_PROJ_SOURCE_DIR/hal/MSPM0Gxx0x/hal.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/hal">
        </file>
        <file path="ORIG_PROJ_SOURCE_DIR/hal/MSPM0Gxx0x/hal.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/hal">
        </file>   
        <file path="ORIG_PROJ_SOURCE_DIR/ADS131M/ADS131M0x.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/ADS131M08">
        </file>
        <file path="ORIG_PROJ_SOURCE_DIR/ADS131M/ADS131M0x.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/ADS131M08">
        </file>  
        <file path="ORIG_PROJ_SOURCE_DIR/ADS131M/ADS131M08_Defaults.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/ADS131M08">
        </file>  
        <file path="ORIG_PROJ_SOURCE_DIR/ADS131M/ADS131M08_Defaults.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/ADS131M08">
        </file>  
        <file path="ORIG_PROJ_SOURCE_DIR/metrology" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules">
        </file>
        <file path="ORIG_PROJ_SOURCE_DIR/DLT645" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules">
        </file>  
  
    </project>
</projectSpec>