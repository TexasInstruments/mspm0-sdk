<?xml version="1.0" encoding="UTF-8"?>
<projectSpec>
    <applicability>
        <when>
            <context
                deviceFamily="ARM"
                deviceId="Cortex M.MSPM0C1106"
            />
        </when>
    </applicability>

    <project
        title="drv8889-q1evm-gui-firmware_LP_MSPM0C1106"
        name="drv8889-q1evm-gui-firmware_LP_MSPM0C1106_nortos_ticlang"
        configurations="Debug"
        toolChain="TICLANG"
        connection="TIXDS110_Connection.xml"
        device="Cortex M.MSPM0C1106"
        ignoreDefaultDeviceSettings="true"
        ignoreDefaultCCSSettings="true"
        products="MSPM0-SDK;sysconfig"

        compilerBuildOptions="
            -I${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/ti/motor_control_stepper/modules/hal/MSPM0C1106/
            -I${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/ti/motor_control_stepper/modules/motor_driver/drv8889-q1/
            -I${ORIG_PROJ_ROOT_DIR}/
            -I${PROJECT_ROOT}
            -I${PROJECT_ROOT}/${ConfigName}
            -D__MSPM0C1106__
            -O2
            -I${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/third_party/CMSIS/Core/Include
            -I${COM_TI_MSPM0_SDK_INSTALL_DIR}/source
            -gdwarf-3
            -mcpu=cortex-m0plus
            -march=thumbv6m
            -mfloat-abi=soft
            -mthumb
        "
        linkerBuildOptions="
            -l${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/ti/driverlib/lib/ticlang/m0p/mspm0c1105_c1106/driverlib.a
            -l${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/ti/iqmath/lib/ticlang/m0p/rts/mspm0c1105_c1106/iqmath.a
            -Wl,--stack_size=512
            -L${PROJECT_BUILD_DIR}/syscfg
            -Wl,--rom_model
            -Wl,--warn_sections
            -L${CG_TOOL_ROOT}/lib
            -llibc.a
        "
        enableSysConfigTool="true"
        sysConfigBuildOptions="
            --product ${COM_TI_MSPM0_SDK_INSTALL_DIR}/.metadata/product.json
            --compiler ticlang
        "
        sourceLookupPath="${COM_TI_MSPM0_SDK_INSTALL_DIR}/source/ti/driverlib"
        description="drv8889-q1evm-gui">

        <pathVariable name="ORIG_PROJ_ROOT_DIR" path="../" scope="project"/>
        <property name="buildProfile" value="release"/>
        <property name="isHybrid" value="true"/>
        <file path="../main.c" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>                
        <file path="../drv8889-q1evm.syscfg" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../ticlang/mspm0c1106.cmd" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../ticlang/startup_mspm0c1105_c1106_ticlang.c" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../drv8889-q1evm-gui.c" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>                
        <file path="../drv8889-q1evm-gui.h" openOnCreation="false" excludeFromBuild="false" action="copy">
        </file>
        <file path="../../../../../../source/ti/motor_control_stepper/modules/hal/MSPM0C1106/hal.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/hal">
        </file>
        <file path="../../../../../../source/ti/motor_control_stepper/modules/hal/MSPM0C1106/hal.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/hal">
        </file>
        <file path="../../../../../../source/ti/motor_control_stepper/modules/motor_driver/drv8889-q1/drv8889-q1.c" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/drv8889-q1">
        </file>
        <file path="../../../../../../source/ti/motor_control_stepper/modules/motor_driver/drv8889-q1/drv8889-q1.h" openOnCreation="false" excludeFromBuild="false" action="copy" targetDirectory="modules/drv8889-q1">
        </file>
    </project>
</projectSpec>