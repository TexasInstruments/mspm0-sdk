%       let nameStr = "#define "+ "DAC12";

/* Defines for DAC12 */
`(nameStr+"_IRQHandler").padEnd(40," ") + (flavor+"_IRQHandler").padStart(40," ")`
`(nameStr+"_INT_IRQN").padEnd(40," ") + ("("+flavor+"_INT_IRQn)").padStart(40," ")`
%       let gpioStr = "GPIO_"+"DAC12";
%       if (inst.dacOutputPinEn){
